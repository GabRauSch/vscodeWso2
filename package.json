{
  "name": "wso2-vscode-extension",
  "displayName": "wso2 VsCode Extension",
  "description": "an extension for developing with wso2 in visual studio code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "propertyExtension.retrieveProperties",
        "category": "propertyExtension",
        "title": "getProperties"
      },
      {
        "command": "wso2-vscode-extension.generateRegistry",
        "category": "wso2",
        "title": "generateRegistry"
      },
      {
        "command": "wso2-vscode-extension.createLog",
        "category": "wso2",
        "title": "Log"
      },
      {
        "command": "wso2-vscode-extension.inputUser",
        "category": "wso2",
        "title": "Input"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/pluralize": "^0.0.29",
    "@types/prettier": "^2.7.3",
    "@types/vscode": "^1.79.0",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "pluralize": "^8.0.0",
    "prettier": "^2.8.8",
    "xml2js": "^0.6.0"
  }
}
